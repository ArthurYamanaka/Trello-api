{
  "version": 2,
  "builds": [
    {
      "src": "api/ticktick/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/createCardSmart",
      "dest": "api/createCardSmart.js"
    },
    {
      "src": "/api/getBoards",
      "dest": "api/getBoards.js"
    },
    {
      "src": "/api/getLists",
      "dest": "api/getLists.js"
    },
    {
      "src": "/api/getCards",
      "dest": "api/getCards.js"
    },
    {
      "src": "/api/openapi",
      "dest": "api/openapi/index.js"
    },

    // Rotas TickTick
    {
      "src": "/api/ticktick/createTaskWithSubtasks",
      "dest": "api/ticktick/createTaskWithSubtasks.js"
    },
    {
      "src": "/api/ticktick/getTasks",
      "dest": "api/ticktick/getTasks.js"
    }
  ]
}
